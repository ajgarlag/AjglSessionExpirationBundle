{
    "name": "ajgl/session-expiration-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to block idle sessions",
    "keywords": ["symfony", "session", "expiration", "idle", "bundle"],
    "homepage": "https://github.com/ajgarlag/AjglSessionExpirationBundle",
    "license": "MIT",
    "authors": [
      {
        "name": "Antonio J. Garc√≠a Lagar",
        "email": "aj@garcialagar.es",
        "homepage": "http://aj.garcialagar.es",
        "role": "developer"
      }
    ],
    "autoload": {
        "psr-4": {
            "Ajgl\\Bundle\\SessionExpirationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ajgl\\Bundle\\SessionExpirationBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "ajgl/session-expiration": "0.1.*",
        "symfony/security-bundle": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/config": "~2.3",
        "symfony/yaml": "~2.3",
        "symfony/dom-crawler": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3",
        "symfony/validator": "~2.3",
        "symfony/browser-kit": "~2.3",
        "symfony/form": "~2.3",
        "matthiasnoback/symfony-config-test": "0.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
